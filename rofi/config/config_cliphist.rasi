configuration {
    modi: "drun,run";
    show-icons: true;
    matching: "prefix";
    //disable-history: true;
    sort: true;
    sorting-method: "fzf";
    kb-accept-entry: "Return,KP_Enter";
    timeout {
        action: "kb-cancel";
        delay:  0;
    }
    dpi: 100;
    filebrowser {
        directories-first: true;
        sorting-method: "name";
    }
}

